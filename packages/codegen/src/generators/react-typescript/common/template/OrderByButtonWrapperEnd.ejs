<% if (withOrderBy) { -%>
    </Col>
    <Col>
      <Select
        value={JSON.stringify(sortValue)}
        className="sort-by-select-width"
        allowClear
        placeholder={intl.formatMessage({ id: "sort.sortBy" })}
        onChange={sortBy => onApplySort(sortBy && JSON.parse(sortBy))}
        options={sortBySelectorOptions}
      />
    </Col>
  </Row>
<% } -%>