<Form.Item name='<%= attr.name %>'
           label='<%= attr.displayName %>'
           <% if (attr?.type === 'Boolean') { -%>
             valuePropName='checked'
             initialValue={false}
        <% } -%>
>
  <% if (attr.type === 'Int') { -%>
    <InputNumber
      type={"number"}
      precision={0}
      max={2147483647}
      min={-2147483648}
    />
  <% } else if (attr.type === 'Float') { -%>
    <InputNumber type={"number"} />
  <% } else if (attr.type === 'String') { -%>
    <Input />
  <% } else if (attr.type === 'Boolean') { -%>
    <Checkbox />
  <% } else if (attr.type === 'ID') { -%>
  <% } else if (attr.enumOptions != null) { -%>
    <Select>
      <% attr.enumOptions.forEach(opt => { -%>
        <Select.Option value="<%= opt.value %>"><%= opt.name %></Select.Option>
      <% }) -%>
    </Select>
  <% } else if (attr.isRelationField) { %>
    <EntityLookupField getDisplayName={get<%= capitalizeFirst(attr.type) %>DisplayName}
                       label="<%= attr.displayName %>"
                       // TODO Uncomment the code and specify the list component
                       // lookupComponent={YourEntityListComponentName}
    />
  <% } else if (attr.type === 'Date') { %>
    <DatePicker />
  <% } else if (attr.type === 'BigInteger') { %>
    <InputNumber type={"number"} precision={0} stringMode={true}/>
  <% } else if (attr.type === 'Long') { %>
    <InputNumber type={"number"} precision={0} stringMode={true}/>
  <% } else if (attr.type === 'BigDecimal') { %>
    <InputNumber type={"number"} stringMode={true}/>
  <% } else { -%>
    <Input />
  <% } -%>
</Form.Item>
